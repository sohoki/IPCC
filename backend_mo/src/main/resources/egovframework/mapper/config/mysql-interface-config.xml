<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	
	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="cacheEnabled" value="true"/>	
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>
	
	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="lmap" type="com.system.ipcc.cti.util.InterfaceLowerKeyMap"/>
		<typeAlias alias="camelmap" type="com.system.ipcc.cti.util.CamelKeyMap"/>
		
		
		<typeAlias alias="NexusAgentComboInfo"  type="com.system.ipcc.cti.nexus.models.dto.NexusAgentComboInfo" />
		<typeAlias alias="NexusAgentRequestInfoDto"  type="com.system.ipcc.cti.nexus.models.dto.NexusAgentRequestInfoDto" />
<!-- 		<typeAlias alias="NexusAgentComboInfo"  type="com.system.ipcc.cti.nexus.models.dto.NexusAgentComboInfo" /> -->
		<typeAlias alias="NexusSubEmployeeInfo"  type="com.system.ipcc.cti.nexus.models.dto.NexusSubEmployeeInfo" />
		<typeAlias alias="NexusSkillRequestInfo"  type="com.system.ipcc.cti.nexus.models.dto.NexusSkillRequestInfo" />
		<typeAlias alias="NexusAgentInfoResponseDto"  type="com.system.ipcc.cti.nexus.models.dto.NexusAgentInfoResponseDto" />
		
		<typeAlias alias="NexusSkillInfo"  type="com.system.ipcc.cti.nexus.models.NexusSkillInfo" />
		<typeAlias alias="NexusDnInfo"  type="com.system.ipcc.cti.nexus.models.NexusDnInfo" />
		<typeAlias alias="NexusDnMajorInfo"  type="com.system.ipcc.cti.nexus.models.NexusDnMajorInfo" />
		<typeAlias alias="NexusDnSubInfo"  type="com.system.ipcc.cti.nexus.models.NexusDnSubInfo" />
		
		<typeAlias alias="NexusGroupInfo"  type="com.system.ipcc.cti.nexus.models.NexusGroupInfo" />
		<typeAlias alias="NexusSkillEmployeeInfo"  type="com.system.ipcc.cti.nexus.models.NexusSkillEmployeeInfo" />
		<typeAlias alias="NexusTeamInfo"  type="com.system.ipcc.cti.nexus.models.NexusTeamInfo" />
		<typeAlias alias="NexusTenantInfo"  type="com.system.ipcc.cti.nexus.models.NexusTenantInfo" />
		<typeAlias alias="NexusDnSubInfo"  type="com.system.ipcc.cti.nexus.models.NexusDnSubInfo" />
		
		<typeAlias alias="NexusLoginIdInfo"	type="com.system.ipcc.cti.nexus.models.NexusLoginIdInfo" />
		
		
		<package name="com.system.ipcc.cti.nexus.models"/>
		<package name="com.system.ipcc.cti.nexus.models.dto"/>
	</typeAliases>
    
</configuration>